\pset linestyle unicode
\pset border 1
\pset format wrapped
\set PROMPT1 '%[%033[1m%]%M %n@%/%R%[%033[0m%]%# '
\set PROMPT1 '%[%033[38;5;27m%]%`hostname -s`%[%033[38;5;102m%]/%/ %[%033[31;5;27m%]%`${HOME}/scripts/custom/psql_role.sh`%[%033[0m%] %# '
\set PROMPT2 '[more] %R > '
\pset null '[NULL]'
\pset pager always
\x auto
\set VERBOSITY verbose
\timing
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper
